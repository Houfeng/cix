{
  "handlers": {
    "^/": "$./handlers/mvc"
  },
  "filters": {
    "^/": "../node_modules/nokit-pjax",
    "^/:auth": "./filters/auth"
  },
  "mvc": {
    "routes": {
      "/auth": {
        "target": "./auth",
        "ignoreAuth": true
      },
      "/": "./main",
      "/setting": "./setting",
      "/projects/{project:^[a-zA-Z0-9\\_\\-]+$}/jobs/{job:^[a-zA-Z0-9\\_\\-]+$}/records/{sn:^[0-9]+$}/console": "./main console",
      "/projects/{project:^[a-zA-Z0-9\\_\\-]+$}/jobs/{job:^[a-zA-Z0-9\\_\\-]+$}/records/{sn:^[0-9]+$}": "./main",
      "/projects/{project:^[a-zA-Z0-9\\_\\-]+$}/jobs/{job:^[a-zA-Z0-9\\_\\-]+$}": "./main",
      "/projects/{project:^[a-zA-Z0-9\\_\\-]+$}": "./main",
      "/api/projects/{project:^[a-zA-Z0-9\\_\\-]+$}/jobs/{job:^[a-zA-Z0-9\\_\\-]+$}/trigger": "./api trigger"
    }
  },
  "folders": {
    "public": {
      "^/bootstrap(.*)": "../node_modules/bootstrap/dist",
      "^/jquery(.*)": "../node_modules/jquery/dist",
      "^/font-awesome(.*)": "../node_modules/font-awesome",
      "^/pjax/(.*)$": "../node_modules/nokit-pjax/client",
      "^/js-yaml/(.*)$": "../node_modules/js-yaml/dist"
    }
  },
  "session": {
    "timeout": 7200
  }
}