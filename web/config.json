{
  "handlers": {
    "^/": "$./handlers/mvc"
  },
  "filters": {
    "^/": "../node_modules/nokit-pjax"
  },
  "mvc": {
    "routes": {
      "/": "./main",
      "/{project:^[a-zA-Z0-9\\_\\-]+$}/{job:^[a-zA-Z0-9\\_\\-]+$}/trigger": "./trigger",
      "/{project:^[a-zA-Z0-9\\_\\-]+$}/{job:^[a-zA-Z0-9\\_\\-]+$}/{sn:^[0-9]+$}": "./main",
      "/{project:^[a-zA-Z0-9\\_\\-]+$}/{job:^[a-zA-Z0-9\\_\\-]+$}": "./main",
      "/{project:^[a-zA-Z0-9\\_\\-]+$}": "./main"
    }
  },
  "folders": {
    "public": {
      "^/bootstrap(.*)": "../node_modules/bootstrap/dist",
      "^/jquery(.*)": "../node_modules/jquery/dist",
      "^/font-awesome(.*)": "../node_modules/font-awesome",
      "^/pjax/(.*)$": "../node_modules/nokit-pjax/client"
    }
  }
}